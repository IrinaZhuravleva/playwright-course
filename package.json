

{
  "version": "0.2.0",
    "main": "index.js",
  "scripts": {
    "test": "playwright test --headed"
  },
  "configurations": [
    {
      "name": "Playwright Test",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/tests/example.test.js",
      "args": ["--project=chromium"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PWDEBUG": "1"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ],
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
