{
  "version": "0.2.0",
  "main": "index.js",
  "scripts": {
    "test": "playwright test --headed"
  },
  "configurations": [
    {
      "name": "Playwright Test",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/tests/example.test.js",
      "args": [
        "--project=chromium"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PWDEBUG": "1"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ],
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-fetch": "^2.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@types/node": "^20.14.10"
  }
}
